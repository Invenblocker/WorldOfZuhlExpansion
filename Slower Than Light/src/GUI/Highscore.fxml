            <ColumnConstraints hgrow="SOMETIMES" maxWidth="454.0" minWidth="10.0" prefWidth="327.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="584.0" minWidth="10.0" prefWidth="306.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="160.0" minWidth="0.0" prefWidth="60.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="163.0" minHeight="10.0" prefHeight="148.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="186.0" minHeight="6.0" prefHeight="66.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="388.0" minHeight="10.0" prefHeight="254.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ListView fx:id="handelHighscoreLw" blendMode="MULTIPLY" prefHeight="271.0" prefWidth="308.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
